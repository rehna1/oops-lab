import java.util.*;
class student
{
public static void main(String[] args)
{
 int age;
 float salary;
 String name;
Scanner s1=new Scanner(System.in);
System.out.println("enter your age\n"+"enter your salary\n"+"enter your name\n");
age=s1.nextInt();
salary=s1.nextFloat();
name=s1.nextLine();
System.out.println("age is"+age+"\nsalary is"+salary+"\nname is"+name);
}
}
